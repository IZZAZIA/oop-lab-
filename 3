package names;

public class Names {

    public static void main(String[] args) {
        // TODO code application logic here
        String[] names={"Elma","Saiqa","Sobia","fatima","Ayesha"};
        for(int i=0;i<names.length;i++){
            System.out.println(names[i]);
        }
    }
    
}
